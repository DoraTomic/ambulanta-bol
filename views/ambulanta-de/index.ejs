<% layout('./boilerplates/de/boilerplate_index') %>
    <div id="carouselExample" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-indicators">
            <button type="button" data-bs-target="#carouselExample" data-bs-slide-to="0" class="active"
                aria-current="true" aria-label="Slide 1"></button>
            <button type="button" data-bs-target="#carouselExample" data-bs-slide-to="1" aria-label="Slide 2"></button>
            <button type="button" data-bs-target="#carouselExample" data-bs-slide-to="2" aria-label="Slide 3"></button>
            <button type="button" data-bs-target="#carouselExample" data-bs-slide-to="3" aria-label="Slide 4"></button>
            <button type="button" data-bs-target="#carouselExample" data-bs-slide-to="4" aria-label="Slide 5"></button>
        </div>
        <div class="carousel-inner">
            <div class="carousel-item gradient-img active">
                <img src="/images/amb/img29.jpg" class="d-block w-100 img-fluid" alt="Čekaonica">

                <div class="carousel-caption d-block text-white lead">
                    <p><em>Gesundheitsfürsorge für Kinder und Erwachsene</em></p>
                </div>
            </div>
            <div class="carousel-item">
                <div class="gradient-img"></div>
                <img src="/images/amb/img30.jpg" class="d-block w-100 img-fluid" alt="Liječnička ordinacija">

                <div class="carousel-caption d-block text-white lead">
                    <p><em>Kontaktieren Sie uns vertrauensvoll</em></p>
                </div>
            </div>
            <div class="carousel-item gradient-img lead">
                <img src="/images/amb/img26.jpg" class="d-block w-100 img-fluid" alt="Previjalište">
                <div class="carousel-caption  d-block text-white lead">
                    <p><em>Für einen unbeschwerten Sommer</em></p>
                </div>
            </div>
            <div class="carousel-item gradient-img lead">
                <img src="/images/amb/img3.jpg" class="d-block w-100 img-fluid" alt="Pogled">
                <div class="carousel-caption d-block text-white lead">
                    <p><em>Im Zentrum des Ortes oberhalb der Apotheke</em></p>
                </div>
            </div>
            <div class="carousel-item gradient-img lead">
                <img src="/images/amb/img40.jpg" class="d-block w-100 img-fluid" alt="Pogled">
                <div class="carousel-caption d-block text-white lead">
                    <p><em>Eine Terminvereinbarung ist nicht erforderlich</em></p>
                </div>
            </div>
        </div>
    </div>
    <%# tekst od tima %>
        <div id="ostatak" class="container my-5 mx-auto">
            <div class="row justify-content-center">
                <div class="col-md-auto">
                    <figure class="text-center">
                        <blockquote class="blockquote">
                            <p class="karma-light lead"><em>Wir sind bestrebt, unseren Patienten, Saisonarbeitern, <br>
                                    sowie in- und ausländischen
                                    Touristen die beste Gesundheitsversorgung anzubieten. <br>
                                    Kontaktieren Sie uns vertrauensvoll. <br>
                                    Wir sind für dich Sie da.</em></p>
                        </blockquote>
                        <figcaption class="blockquote-footer float-end">
                            Ihr Ärzte-Team
                        </figcaption>
                    </figure>
                </div>
            </div>
        </div>